{
  "version": "1.0",
  "defaultSchema": "CSV",
  "schemas": [
    {
      "name": "CSV",
      "type": "custom",
      "factory": "com.scindapsus.calcite.csv.CsvSchemaFactory",
      "operand": {
        "directory": "csv"
      }
    },
    {
      "name": "CSV2",
      "type": "custom",
      "factory": "com.scindapsus.calcite.csv.CsvSchemaFactory",
      "operand": {
        "directory": "csv2"
      }
    },
    {
      "name": "MY",
      "type": "custom",
      "factory": "org.apache.calcite.adapter.jdbc.JdbcSchema$Factory",
      "operand": {
        "jdbcDriver": "com.mysql.cj.jdbc.Driver",
        "jdbcUrl": "jdbc:mysql://localhost:3306/ds1",
        "jdbcUser": "root",
        "jdbcPassword": "root"
      }
    }
  ]
}